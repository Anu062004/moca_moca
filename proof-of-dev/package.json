    "@react-three/fiber": "^9.4.0",
    "@tanstack/react-query": "^5.90.2",
    "@types/three": "^0.180.0",
    "clsx": "^2.1.1",
    "ethers": "^6.15.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.545.0",
